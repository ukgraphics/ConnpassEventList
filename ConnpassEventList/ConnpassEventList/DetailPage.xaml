<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ConnpassEventList.DetailPage">
    <ContentPage.Padding>
        <OnPlatform x:TypeArguments="Thickness"
      iOS="10, 20, 10, 0" Android="10, 20, 10, 0" WinPhone="10, 20, 10, 0" />
    </ContentPage.Padding>
    <ContentPage.Content>
        <ScrollView>
            <StackLayout>
                <WebView x:Name="detail" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand"  />
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>